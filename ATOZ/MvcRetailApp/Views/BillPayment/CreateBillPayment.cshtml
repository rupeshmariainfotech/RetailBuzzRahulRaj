                    $("#PaymentTypeView").html(data);
                }
            });
        }
        else if (type == 'Cheque') {
            $.ajax({
                url: "/BillPayment/ChequePayment",
                type: "GET",
                success: function (data) {
                    $("#PaymentTypeView").html(data);
                }
            });
        }
        else if (type == 'CashCardCheque') {
            $.ajax({
                url: "/BillPayment/CashCardChequePayment",
                type: "GET",
                success: function (data) {
                    $("#PaymentTypeView").html(data);
                }
            });
        }
    }
