@model CodeFirstEntities.MainApplication
@{
    ViewBag.Title = "BrandDeletePartial";
}

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model=>model.BrandMasterDetails.Id)
        @Html.HiddenFor(model=>model.BrandMasterDetails.BrandName)
        @Html.HiddenFor(model=>model.BrandMasterDetails.BrandCode)

    <h2><strong>Brand </strong>Details</h2>
    <br />

    <div class="col-md-12 col-lg-12">
        <div class="col-md-12 col-lg-12">
            <div class="row">
                <div class="form-group col-md-4 col-lg-4">
                    <label for="exampleInputname">Brand Name</label>
                </div>
                <div class="form-group col-md-4 col-lg-4">
                    @Html.DisplayFor(model => model.BrandMasterDetails.BrandName, new { @class = "form-control" })
                </div>
            </div>

            <div class="row">
                <div class="col-lg-offset-2">
                    <button type="submit" class="btn btn-success" onclick="DeleteBrandPartial()">Delete</button>
                </div>
            </div>
        </div>
    </div>