<script type="text/javascript">
    $(function () {
        var temp = $("#date").text();
        var dat = new Date(temp);
        var month = ["JAN", "FEB", "MAR", "APR", "MAY", "JUN", "JUL", "AUG", "SEP", "OCT", "NOV", "DEC"];
        var data = dat.getDate() + "/" + month[dat.getMonth()] + "/" + dat.getFullYear();
        document.getElementById('date').innerHTML = data;
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {

        $("#outwardno").autocomplete({
            minLength: 1,
            source: '/Inward/GetOutwardNosInterGodown',
            select: function (event, ui) {
                if (ui.item) {
                    $("#outwardno").val(ui.item.value);
                    var outwardno = $("#outwardno").val();

                    $.getJSON("/Inward/GetOutwardInterGodownDetails", { Outwardno: outwardno }, function (data) {
                        if (data != null) {
                            document.getElementById("preparebydetails").style.display = "inline";
                            document.getElementById("ShopDetails").style.display = "inline";
                            document.getElementById("ShopNo&Details").style.display = "inline";
                            document.getElementById("Salesman").style.display = "inline";


                            $("#GdName").text(data.FromGodownname);
                            $("#GdNumber").text(data.FromGodownContactNo);
                            $("#GdEmail").text(data.FromGodownEmail);

                            $("#InwardInterGodownDetails_FromGodownCode").val(data.FromGodownCode);
                            $("#InwardInterGodownDetails_FromGodownName").val(data.FromGodownname);
                            $("#InwardInterGodownDetails_FromGodownEmail").val(data.FromGodownEmail);
                            $("#InwardInterGodownDetails_FromGodownContactNo").val(data.FromGodownContactNo);
                            $("#InwardInterGodownDetails_Narration").val(data.Narration);
                            if (data.Narration != null) {
                                document.getElementById("Narr").style.display = "inline";
                                document.getElementById("narr").innerHTML = data.Narration;
                            }
                            $("#skname").text(data.PrepareBy);
                            $("#skemail").text(data.PrepareByEmail);
                            $("#skcontact").text(data.PrepareByContactNo);

                            $("#InwardInterGodownDetails_StoreKeeperName").val(data.PrepareBy);
                            $("#InwardInterGodownDetails_StoreKeeperContact").val(data.PrepareByContactNo);
                            $("#InwardInterGodownDetails_StoreKeeperEmail").val(data.PrepareByEmail);



                            $("#outwardgatepass").val(data.GatePass);

                            var godowncode = document.getElementById("godowncode").value;
                            $.getJSON('/Inward/GetStoreKeeperDetails', { GodownCode: godowncode }, function (data) {
                                if (data != null) {
                                    $("#InwardInterGodownDetails_ToGodownCode").val(godowncode);
                                    $("#InwardInterGodownDetails_ToGodownName").val(data.GodownName);
                                    $("#InwardInterGodownDetails_ToGodownEmail").val(data.GodownEmail);
                                    $("#InwardInterGodownDetails_ToGodownContactNo").val(data.ContactNo1);
                                }
                            });

                            var email = $("#preparedby").val();
                            $.getJSON("/Inward/GetEmployeeDetails", { Email: email },
                                function (data) {
                                    $("#name").text(data.Name);
                                    $("#InwardInterGodownDetails_EmpName").val(data.Name);
                                    $("#designation").text(data.Designation);
                                    $("#InwardInterGodownDetails_EmpDesignation").val(data.Designation)
                                    $("#email").text(email);
                                    $("#InwardInterGodownDetails_EmpEmail").val(email);
                                });
                            $("#gatepass").attr("disabled", false);
                        }
                    });
                }
            }
        });
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {
        $("#Print").click(function () {
            window.location.assign("/Inward/InwardInterGodownPrint");
        });

        $("#gatepass").change(function () {
            var gatepass = $(this).val();
            var outwardgatepass = document.getElementById("outwardgatepass").value;

            if (gatepass != "") {
                if (gatepass == outwardgatepass) {
                    $("#Submit").attr("disabled", false);
                    var outwardno = document.getElementById("outwardno").value;

                    $.ajax({
                        url: '/Inward/GetItemListInterGodown',
                        data: { OutwardNo: outwardno },
                        type: 'Get',
                        success: function (data) {
                            if (data != null) {
                                $("#LoadList").html(data);
                            }
                        }
                    });
                }
                else {
                    $("#Submit").attr("disabled", true);
                }
            }
        });
        $("#Submit").click(function () {
            setTimeout(function () {
                var errorcount = 0;
                if ($("#outwardno").val() == "" || $("#gatepass").val() == "") {
                    errorcount++;
                }

                if (errorcount == 0) {
                    $("#Submit").attr("disabled", true);
                }
                else {
                    return false;
                }
            }, 500);
        });
    });
</script>


