                alertbox("Please Select Item First");
                $("#itemCode1").val("").attr("selected", true);
                document.getElementById("quantity1").value = 0.000;
                document.getElementById("amount1").innerHTML = 0.00;
                $("#description1").val("");
                $("#rate1").val(0.00);
