@model CodeFirstEntities.MainApplication
@{
    ViewBag.Title = "getTempItemDetails";
}

    
                          
 @{
              //int rowId = 1;
              if(Model.itemTempDetailList != null && Model.itemTempDetailList.Any())
              {
                  <div class="panel">
                  <div class="form-group"></div>
                      <div class='form-group col-md-3 col-lg-3'>
                          <label for="exampleInputDistrict">Barcode</label>
                      </div>
                      <div class="form-group col-md-3 col-lg-3">
                          <label for="exampleInputDistrict">Item Name</label>
                      </div>
                      <div class="form-group col-md-3 col-lg-3">
                          <label for="exampleInputDistrict">Item Rate</label>
                      </div>
                      <div class="form-group col-md-3 col-lg-3">
                          <label for="exampleInputDistrict">Item Description</label>
                      </div>
                  @foreach(var item in Model.itemTempDetailList)
                  {
                      string imageUrl = item.barcode; 
                      <div class="row">
                          <div class="col-md-12 col-lg-12">
                              @*<div class='form-group col-md-3 col-lg-3'>
                                  <img id="barcode" src="@imageUrl" alt="" />
                              </div>*@
                              <div class="form-group col-md-3 col-lg-3">
                                  </div>
                              <div class="form-group col-md-3 col-lg-3">
                                  @Html.DisplayFor(modelitem => item.itemName)
                              </div>
                              <div class="form-group col-md-3 col-lg-3">
                                  @Html.DisplayFor(modelitem => item.costprice)
                              </div>
                              <div class="form-group col-md-3 col-lg-3">
                                  @Html.DisplayFor(modelitem => item.description)
                              </div>
                          </div>
                      </div>
                  }
                       </div>
              }

          }
   