                    <div class="col-md-4 col-lg-4">
                         @Html.DropDownListFor(model => model.GodownDetails.GodownName, new SelectList(Model.GodownMasterList, "GdCode", "GodownName"), "Select Godown Name", new { @class = "form-control", id = "DDLGodown" })
                    </div>
