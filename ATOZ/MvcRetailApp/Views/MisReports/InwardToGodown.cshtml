                            <label>To Date<i style="color: red">*</i></label>
                        </div>
                        <div class="col-md-8 col-lg-8">
                            @Html.TextBoxFor(model => model.InwardFromStockDistributionDetails.ModifiedOn, new { id = "todate", @class = "form-control", @placeholder = "To Date" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-lg-12">
                    <div style="height: 10px"></div>
                </div>

                <div class="col-md-12 col-lg-12">
