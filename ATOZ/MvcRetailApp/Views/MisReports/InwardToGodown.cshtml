<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/base/jsDatePick_ltr.min.css")" />
<script type="text/javascript" src="@Url.Content("~/Scripts/jsDatePick.min.1.3.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jsDatePick.jquery.min.1.3.js")"></script>

<script type="text/javascript">
    jQuery(document).ready(function () {
        var data1 = new JsDatePick({
            useMode: 2,
            target: "fromdate",
            dateFormat: "%d/%M/%Y"
        });
        data1.addOnSelectedDelegate(function () {
            var fromdate = document.getElementById("fromdate").value;
            var todate = document.getElementById("todate").value;
            if (todate != "") {
                $.ajax({
                    url: '/MisReports/GetInwardToGodownDetailsByDate',
                    type: "GET",
                    data: { FromDate: fromdate, ToDate: todate },
                    success: function (data) {
                        $("#InwardPartialView").html(data);
                    }
                })
            }
        });
        var data2 = new JsDatePick({
            useMode: 2,
            target: "todate",
            dateFormat: "%d/%M/%Y"
        });
        data2.addOnSelectedDelegate(function () {
            var fromdate = document.getElementById("fromdate").value;
            var todate = document.getElementById("todate").value;
            if (fromdate != "") {
                $.ajax({
                    url: '/MisReports/GetInwardToGodownDetailsByDate',
                    type: "GET",
                    data: { FromDate: fromdate, ToDate: todate },
                    success: function (data) {
                        $("#InwardPartialView").html(data);
                    }
                })
            }
        });
    });
</script>

@*OPEN PRINT PAGE ON PRINT CLICK BUTTON*@
<script type="text/javascript">
    function PrintDetails() {
        window.open("/MisReports/PrintInwardToGodownDatewise", "_target", "width=715,height=830,top=50px,left=300px");
    }
</script>

@Html.Partial("../Shared/Header")
<!-- End: Header -->

<!-- Start: Sidebar -->
<section>
    <div class="mainwrapper">
        @Html.Partial("../Shared/LeftMenu")
        <!-- End: Sidebar -->

        <div class="mainpanel">
            <div class="col-md-12 col-lg-12">

                <div style="height: 15px"></div>

                <label style="font-size: 18px">Inward From Stock Distribution Report</label>
                <div style="height: 10px"></div>

                <div class="col-md-12 col-lg-12">
                    <div class="col-md-4 col-lg-4">
                        <div class="col-md-4 col-lg-4">
                            <div class="row">
                                <label>From Date<i style="color: red">*</i></label>
                            </div>
                        </div>
                        <div class="col-md-8 col-lg-8">
                            @Html.TextBoxFor(model => model.InwardFromStockDistributionDetails.ModifiedOn, new { id = "fromdate", @class = "form-control", @placeholder = "From Date" })
                        </div>
                    </div>

                    <div class="col-md-4 col-lg-4">
