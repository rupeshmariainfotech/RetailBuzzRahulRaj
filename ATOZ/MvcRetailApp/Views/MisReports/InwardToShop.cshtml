<script type="text/javascript" src="@Url.Content("~/Scripts/jsDatePick.jquery.min.1.3.js")"></script>

<script type="text/javascript">
    function Report(type) {
        if (type == 'Date') {
            $.ajax({
                url: "/MisReports/InwardToShopDatewise",
                type: "GET",
                success: function (data) {
                    document.getElementById("LoadSupplier").innerHTML = "";
                    $("#LoadSupplier").html(data);
                }
            });
        }
        else {
            $.ajax({
                url: "/MisReports/InwardToShopGodownwise",
                type: "GET",
                success: function (data) {
                    document.getElementById("LoadSupplier").innerHTML = "";
                    $("#LoadSupplier").html(data);
                }
            });
        }
    }
</script>

@Html.Partial("../Shared/Header")
<!-- End: Header -->

<!-- Start: Sidebar -->
<section>
    <div class="mainwrapper">
        @Html.Partial("../Shared/LeftMenu")
        <!-- End: Sidebar -->

        <div class="mainpanel">
            <div class="col-md-12 col-lg-12">

                <div style="height: 15px"></div>

                <label style="font-size: 18px">Inward To Shop Report</label>
                <div style="height: 10px"></div>


                <div class="col-lg-12 col-md-12">
                    <table style="border: 1px double; width: 100%">
                        <tr>
                            <td>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <div style="height: 8px"></div>
                                        <div class="col-md-4 col-lg-4">
                                            <input type="radio" name="InwardBtn" onclick="Report('Date')" />
                                            <label style="font-size: 14px">&nbsp Datewise Report</label>
                                        </div>
                                        <div class="col-md-4 col-lg-4">
                                            <input type="radio" name="InwardBtn" onclick="Report('Godown')" />
                                            <label style="font-size: 14px">&nbsp Godownwise Report</label>
                                        </div>
                                        <div style="height: 8px"></div>
                                    </div>
                                </div>
                            </td>
                        </tr>

                        <tr>
                            <td>
                                <br />
                                <div id="LoadSupplier"></div>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

