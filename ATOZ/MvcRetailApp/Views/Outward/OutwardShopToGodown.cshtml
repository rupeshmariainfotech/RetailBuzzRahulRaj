                                                                <label>Godown Name<i style="color: red">*</i></label><br />
                                                                @Html.DropDownListFor(model => model.OutwardShopToGodownDetails.GodownCode, new SelectList(Model.GodownMasterList, "GdCode", "GodownName"), "Select Godown", new { id = "godownname", @class = "form-control" })
                                                                @Html.ValidationMessageFor(model => model.OutwardShopToGodownDetails.GodownCode, null, new { style = "color:red" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-2 col-md-2"></div>
                                                <div class="col-lg-6 col-md-6">
                                                    <div class="col-lg-12 col-md-12">
                                                        <div class="row">
                                                            <div class="row">
                                                                <div style="display: none" id="gdaddresses">
                                                                    <label>Godown Area<i style="color: red">*</i></label><br />
                                                                    @Html.DropDownListFor(model => model.GodownAddressDetails.AreaName, Enumerable.Empty<SelectListItem>(), "Select Address", new { id = "godownarea", @class = "form-control" })
                                                                    @Html.ValidationMessageFor(model => model.GodownAddressDetails.AreaName, null, new { style = "color:red" })
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div id="godowndetails" style="display: none">
                                                <div class="col-lg-12 col-md-12">
                                                    <br />
                                                    <div class="row">
                                                        <label>Address: &nbsp;</label><label id="gdaddress" style="font-weight: normal"></label><br />
                                                        <label>Contact No: &nbsp;</label><label id="gdcontact" style="font-weight: normal"></label><br />
                                                        <label>Email: &nbsp;</label><label id="gdemail" style="font-weight: normal"></label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-3 col-md-3 form-group" style="border-right: 1px solid lightgray; height: 200px">
                                    <div style="height: 10px"></div>
                                    <div class="col-lg-12 col-md-12">
                                        <label>Store Keeper Name</label><br />
                                        @Html.TextBoxFor(model => model.OutwardShopToGodownDetails.StoreKeeperName, new { id = "StoreKeeperName", @placeholder = "Store Keeper Name", @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.OutwardShopToGodownDetails.StoreKeeperName, null, new { style = "color:red" })
                                        <div style="height: 12px"></div>
                                        <div id="empdetailsdiv" style="display: none">
                                            <label>Designation: &nbsp;</label><label id="Designation" style="font-weight: normal"></label><br />
                                            <label>Contact No: &nbsp;</label><label id="MobileNo" style="font-weight: normal"></label><br />
                                            <label>Email: &nbsp;</label><label id="Email" style="font-weight: normal"></label>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-5 col-md-5 form-group">
                                    <div class="col-md-12 col-lg-12">
                                        @{
                                            string date = DateTime.Now.ToString("MM/dd/yyyy");
                                            string outwardcode = Model.OutwardShopToGodownDetails.OutwardCode;
                                        }
                                        <div style="height: 10px"></div>
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6">
                                                <label style="font-size: 14px;">Date:</label>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <div class="row">
                                                    <label id="date">@date</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div style="height: 7px"></div>
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6">
                                                <label style="font-size: 14px;">Outward No:</label>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <div class="row">
                                                    <label>@outwardcode</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div style="height: 7px"></div>
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6">
                                                <label style="font-size: 14px;">Reference No : </label>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <div class="row">
                                                    @Html.TextBoxFor(model => model.OutwardShopToGodownDetails.GatePass, new { @class = "form-control", @autocomplete = "off", @placeholder = "Reference No", @readonly = "readonly" })
                                                    @Html.ValidationMessageFor(model => model.OutwardShopToGodownDetails.GatePass, null, new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="row form-group">
                                    <table id="DynamicRows" class="table" style="border-color: #d2d2d2; border: outset">
                                        <tr>
                                            <th class="col-lg-1 col-md-1">
                                                <label style="text-align: center; width: 100%">S.No.</label>
                                            </th>
                                            <th class="col-lg-3 col-md-3">
                                                <label>Item<i style="color: red">*</i></label>
                                            </th>
                                            <th>
                                                <label>Description</label>
                                            </th>
                                            <th>
                                                <label>Design</label>
                                            </th>
                                            <th>
                                                <label>Unit</label>
                                            </th>
                                            <th>
                                                <label>Color</label>
                                            </th>
                                            <th>
                                                <label>Stock Qty</label>
                                            </th>
                                            <th class="col-lg-1 col-md-1">
                                                <label>Outward Qty<i style="color: red">*</i></label>
                                            </th>
                                            <th>
                                                <label>Current Bal</label>
                                            </th>
                                            <th></th>
                                        </tr>
                                        <tr id="row1">
                                            <td style="text-align: center">
                                                <label style="font-weight: normal">1</label>
                                            </td>
                                            <td>
                                                <select id="ItemCode1" name="ItemCode1" class="form-control">
                                                    <option value="">Select Item </option>
                                                </select>
                                                <input type="hidden" id="ItemName1" name="ItemName1" />
                                                <input type="hidden" id="Barcode1" name="Barcode1" />
                                            </td>
                                            <td>
                                                <label id="Description1" style="font-weight: normal"></label>
                                                <input type="hidden" id="DescriptionValue1" name="DescriptionValue1" />
                                            </td>
                                            <td>
                                                <label id="Design1" style="font-weight: normal"></label>
                                                <input type="hidden" id="DesignValue1" name="DesignValue1" />
                                                <input type="hidden" id="DesignCode1" name="DesignCode1" />
                                            </td>
                                            <td>
                                                <label id="Unit1" style="font-weight: normal"></label>
                                                <input type="hidden" id="UnitValue1" name="UnitValue1" />
                                                <input type="hidden" id="NumberType1" name="NumberType1" />
                                            </td>
                                            <td>
                                                <label id="Color1" style="font-weight: normal"></label>
                                                <input type="hidden" id="ColorValue1" name="ColorValue1" />
                                                <input type="hidden" id="Brand1" name="Brand1" />
                                                <input type="hidden" id="Size1" name="Size1" />
                                                <input type="hidden" id="Rate1" name="Rate1" />
                                                <input type="hidden" id="MRP1" name="MRP1" />
                                                <input type="hidden" id="Material1" name="Material1" />
                                            </td>
                                            <td style="text-align: right">
                                                <label id="StockQuantity1" style="font-weight: normal"></label>
                                                <input type="hidden" id="StockQuantityValue1" name="StockQuantityValue1" />
                                            </td>
                                            <td>
                                                <input type="text" class="form-control" style="text-align: center" id="OutwardQuantity1" name="OutwardQuantity1" onchange="CalculateBalance(1)" disabled="disabled" onkeypress="return AllowNumbers(event)" autocomplete="off" placeholder="Qty" />
                                            </td>
                                            <td style="text-align: right">
                                                <label style="font-weight: normal" id="Balance1"></label>
                                                <input type="hidden" id="BalanceValue1" name="BalanceValue1" />
                                            </td>
                                            <td></td>
                                        </tr>
                                    </table>
                                </div>


                                <div class="col-md-12 col-lg-12 form-group">
                                    <div class="row">
                                        <div class="col-lg-3 col-md-3">
                                            <label>Salesman Name<label style="color: red">*</label></label><br />
                                            @Html.TextBoxFor(model => model.OutwardShopToGodownDetails.SalesmanName, new { id = "SalesmanName", @placeholder = "Salesman Name", @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.OutwardShopToGodownDetails.SalesmanName, null, new { style = "color:red" })
                                        </div>
                                        <div style="height: 12px"></div>
                                        <div id="salesmandetailsdiv">
                                            <div class="col-lg-4 col-md-4">
                                                <label>Designation &nbsp;</label><br />
                                                <label id="SalesmanDesignation" style="font-weight: normal"></label>
                                                <br />
                                            </div>
                                            <div class="col-lg-3 col-md-3">
                                                <label>Contact No &nbsp;</label><br />
                                                <label id="SalesmanMobileNo" style="font-weight: normal"></label>
                                                <br />
                                            </div>
                                            <div class="col-lg-2 col-md-2">
                                                <label>Email &nbsp;</label><br />
                                                <label id="SalesmanEmail" style="font-weight: normal"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-12 col-lg-12">
                                    <div style="height: 15px"></div>
                                    <div class="row">
                                        <div class="col-md-3 col-lg-3">
                                            <label>Prepared By</label><br />
                                            <label id="name" style="font-weight: normal"></label>
                                        </div>

                                        <div class="col-md-4 col-lg-4">
                                            <label>Email-Id</label><br />
                                            <label id="emailid" style="font-weight: normal"></label>
                                        </div>
                                        <div class="col-md-3 col-lg-3">
                                            <label>Designation</label><br />
                                            <label id="designation" style="font-weight: normal"></label>
                                        </div>
                                        @{
                                            string preparetimeid = DateTime.Now.ToShortTimeString();
                                        }
                                        <div class="col-md-2 col-lg-2">
                                            <label>Prepare Time</label><br />
                                            <label style="font-weight: normal">@preparetimeid</label>
                                        </div>
                                    </div>
                                    <div style="height: 40px"></div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-3 col-md-3">
                                        <div class="col-lg-12 col-md-12">
                                            <label>Narration</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-9 col-md-9">
                                        @Html.TextBoxFor(model => model.OutwardShopToGodownDetails.Narration, new { @class = "form-control", @autocomplete = "off", @placeholder = "Narration" })
                                    </div>
                                </div>

                                <div class="col-lg-12 col-md-12">
                                    <div style="height: 10px"></div>
                                </div>

                            </div>
                        </td>
                    </tr>
                </table>

                <div style="height: 10px"></div>

                <div style="text-align: center">
                    <button type="submit" id="Submit" class="btn btn-success">Save</button>
                </div>
                <div style="text-align: right">
                    <div style="height: 3px"></div>
                    <button type="button" id="Delete" class="btn btn-danger" data-toggle="tooltip" title="Delete"><i class="fa fa-trash" title="Delete"></i></button>
                    <button type="button" id="Print" class="btn btn-primary" data-toggle="tooltip" title="Print"><i class="fa fa-print" title="Print"></i></button>
                </div>

            </div>
        </div>
    </div>
</section>
}
