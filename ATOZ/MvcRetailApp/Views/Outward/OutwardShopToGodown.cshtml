<script>
    function CalculateBalance(rowcount) {

        var stkquan = (+document.getElementById("StockQuantity" + rowcount).innerHTML);
        var outquan = (+document.getElementById("OutwardQuantity" + rowcount).value);

        // validation on outward quantity > stock quantity
        if (outquan > stkquan) {
            alertbox("Cannot exceed above Stock Quantity");
            document.getElementById("OutwardQuantity" + rowcount).value = document.getElementById("StockQuantity" + rowcount).innerHTML;
        }

        var balance = (document.getElementById("StockQuantity" + rowcount).innerHTML) - (document.getElementById("OutwardQuantity" + rowcount).value);
        document.getElementById("Balance" + rowcount).innerHTML = balance.toFixed(2);
        document.getElementById("BalanceValue" + rowcount).value = balance;
    }

    function AllowNumbers(key) {
        var keycode = (key.which) ? key.which : key.keyCode;
        if (!(keycode == 8 || keycode == 46) && (keycode < 48 || keycode > 57)) {
            return false;
        }
    }

    function DeleteRow(row) {
        var row = "row" + row;
        confirmbox("Sure You Want To Delete This Row", row);
    }
</script>

<script>
    $(document).keypress(function (e) {
        if (e.which == 13) {

            var rowcount = document.getElementById("rowcount").value;

            
            if ($("#ItemCode" + rowcount).val() == "")
            {
                alertbox("Please Select The Item");
                return false;
            }
            else if ($("#OutwardQuantity" + rowcount).val() == "") {
                alertbox("Please Enter The Outward Quantity");
                return false;
            }

            $("#OutwardQuantity" + rowcount).attr("disabled", true);
            $("#ItemCode" + rowcount).attr("disabled", true);

            rowcount++;
            var row = "row" + rowcount;
            var barcode = "Barcode" + rowcount;
            var itemcode = "ItemCode" + rowcount;
            var itemname = "ItemName" + rowcount;
            var description = "Description" + rowcount;
            var descriptionvalue = "DescriptionValue" + rowcount;
            var design = "Design" + rowcount;
            var designvalue = "DesignValue" + rowcount;
            var color = "Color" + rowcount;
            var colorvalue = "ColorValue" + rowcount;
            var unit = "Unit" + rowcount;
            var unitvalue = "UnitValue" + rowcount;
            var rate = "Rate" + rowcount;
            var mrp = "MRP" + rowcount;
            var material = "Material" + rowcount;
            var brand = "Brand" + rowcount;
            var designcode = "DesignCode" + rowcount;
            var numbertype = "NumberType" + rowcount;
            var size = "Size" + rowcount;
            var stockquantity = "StockQuantity" + rowcount;
            var stockquantityvalue = "StockQuantityValue" + rowcount;
            var outwardquantity = "OutwardQuantity" + rowcount;
            var balance = "Balance" + rowcount;
            var balancevalue = "BalanceValue" + rowcount;

            var itemlist = "<tr id='" + row + "'><td style='text-align:center'>" + rowcount + "</td>";
            itemlist += "<td><select class='form-control' id='" + itemcode + "' name='" + itemcode + "'><option>Select Item</option></select><input type='hidden' name='" + itemname + "' id='" + itemname + "' /><input type='hidden' name='" + barcode + "' id='" + barcode + "' /></td>";
            itemlist += "<td><label id='" + description + "' style='font-weight:normal'>---</label><input type='hidden' id='" + descriptionvalue + "' name='" + descriptionvalue + "' /></td>";
            itemlist += "<td><label id='" + design + "' style='font-weight:normal'>---</label><input type='hidden' id='" + designvalue + "' name='" + designvalue + "'></td>";
            itemlist += "<td><label id='" + unit + "' style='font-weight:normal'>---</label><input type='hidden' id='" + unitvalue + "' name='" + unitvalue + "'></td>";
            itemlist += "<td><label id='" + color + "' style='font-weight:normal'>---</label><input type='hidden' id='" + colorvalue + "' name='" + colorvalue + "'><input type='hidden' id='" + brand + "' name='" + brand + "'><input type='hidden' id='" + size + "' name='" + size + "'><input type='hidden' id='" + designcode + "' name='" + designcode + "'><input type='hidden' id='" + numbertype + "' name='" + numbertype + "'></td>";
            itemlist += "<td style='text-align:right'><label id='" + stockquantity + "' style='font-weight:normal;text-align:right'>---</label><input type='hidden' id='" + stockquantityvalue + "' name='" + stockquantityvalue + "'></td>";
            itemlist += "<td><input type='text' id='" + outwardquantity + "' name='" + outwardquantity + "'  class='form-control' style='text-align:center;font-weight:normal' onchange='CalculateBalance(" + rowcount + ")' placeholder='Qty' onkeypress='return AllowNumbers(event)' /></td>";
            itemlist += "<td style='text-align:right'><label style='font-weight:normal' id='" + balance + "'>---</label><input type='hidden' id='" + balancevalue + "' name='" + balancevalue + "'></td>";
            itemlist += "<td><a href='#' onclick='DeleteRow(" + rowcount + ")' class='fa fa-trash fa-lg' style='color:red' /></a><input type='hidden' id='" + rate + "' name='" + rate + "'><input type='hidden' id='" + mrp + "' name='" + mrp + "'><input type='hidden' id='" + material + "' name='" + material + "'></td></tr>";

            $("#DynamicRows").append(itemlist);
            document.getElementById("rowcount").value = rowcount;

            var shopcode = document.getElementById("shopcode").value;
            $.getJSON("/Outward/GetItemsFromShopStock", { ShopCode: shopcode }, function (data) {

                var select = $("#" + itemcode);
                select.empty();
                select.append($('<option>', {
                    text: "Select Item",
                    value: ""
                }));

                $.each(data, function (i, item) {
                    select.append($('<option>', {
                        value: item.Value,
                        text: item.Text
                    }));
                });
            });

            $(document).ready(function () {

                $("#" + itemcode).change(function () {
                    var itemcode = $(this).val();

                    var counter = 0;
                    for (var i = 1; i < rowcount; i++) {
                        var outwardquant = "OutwardQuantity" + i;
                        if (itemcode == $("#ItemCode" + i).val()) {
                            alertbox("Item Already Selected;Please Change The Quantity");
                            $("#" + outwardquant).attr("disabled", false);
                            $("#ItemCode" + rowcount).val("").attr("selected", true);
                            counter = i;
                            return false;
                        }
                    }

                    if (counter == 0) {

                        for (i = 1; i < rowcount; i++) {
                            var QUANTITY = "OutwardQuantity" + i;
                            //IF ROW IS DELETED THEN QUANTITY WON'T GET ENABLED
                            if (document.getElementById(QUANTITY) != null) {
                                document.getElementById(QUANTITY).disabled = true;
                            }
                        }

                        if (itemcode != "") {
                            var shopcode = document.getElementById("shopcode").value;
                            $.getJSON("/Outward/GetItemDetailsFromShopStock", { ItemCode: itemcode, ShopCode: shopcode }, function (data) {
                                $("#" + barcode).val(data.Barcode);
                                $("#" + description).text(data.Description);
                                $("#" + design).text(data.DesignName);
                                $("#" + unit).text(data.Unit);
                                $("#" + color).text(data.Color);
                                $("#" + stockquantity).text(data.Quantity);
                                document.getElementById(outwardquantity).style.display = "inline";

                                $("#" + itemname).val(data.ItemName)
                                $("#" + descriptionvalue).val(data.Description);
                                $("#" + designvalue).val(data.DesignName);
                                $("#" + unitvalue).val(data.Unit);
                                $("#" + colorvalue).val(data.Color);
                                $("#" + rate).val(data.SellingPrice);
                                $("#" + mrp).val(data.MRP);
                                $("#" + material).val(data.Material);
                                $("#" + stockquantityvalue).val(data.Quantity);
                                $("#" + brand).val(data.Brand);
                                $("#" + size).val(data.Size);
                                $("#" + numbertype).val(data.NumberType);
                                $("#" + designcode).val(data.DesignCode);
                            });
                        }
                        else {
                            $("#" + description).text(data.Description);
                            $("#" + design).text(data.DesignName);
                            $("#" + unit).text(data.Unit);
                            $("#" + color).text(data.Color);
                            $("#" + stockquantity).text(data.Quantity);
                            document.getElementById(outwardquantity).style.display = "none";
                        }
                    }
                });
            });

        }
    });
</script>

<script>
