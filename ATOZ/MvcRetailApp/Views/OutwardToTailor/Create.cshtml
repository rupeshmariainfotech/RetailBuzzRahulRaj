            $.ajax({
                url: "/OutwardToTailor/CardPayment",
                type: "GET",
                success: function (data) {
                    $("#PaymentTypeView").html(data);
                }
            });
        }
        else if (type == 'Cheque') {
            $.ajax({
                url: "/OutwardToTailor/ChequePayment",
                type: "GET",
                success: function (data) {
                    $("#PaymentTypeView").html(data);
                }
            });
        }
        else if (type == 'CashCardCheque') {
            $.ajax({
                url: "/OutwardToTailor/CashCardChequePayment",
                type: "GET",
