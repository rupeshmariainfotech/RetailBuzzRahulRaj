@*CLIENT NAME AUTO COMPLETE*@
<script type="text/javascript">
    $(function () {
        $("#ClientName").autocomplete({
            source: function (request, response) {
                $.getJSON("/OutwardToTailor/GetClientNames", { SearchTerm: request.term },
                function (data) {
                    response(data);
                }
               )
            },

            minLength: 1,
            @*GET RETAIL BILL NO FROM CLIENT NAME*@
            select: function (event, ui) {
                if (ui.item) {
                    $("#ClientName").val(ui.item.value);
                    var client = document.getElementById("ClientName").value;

                    //FILL RETAIL BILL DDL BY CLIENT NAMER
                    $.getJSON("/OutwardToTailor/GetRetailBillFromClient", { ClientName: client },
               function (data) {

                   var select = $("#RetailBillNoDDL");
                   select.empty();
                   $.each(data.RetailBillNos, function (index, itemdata) {
                       select.append($('<option/>', {
                           text: itemdata.Text,
                           value: itemdata.Value,
                       }));
                   });
               });

                    //FILL SALES BILL DDL BY CLIENT NAMER
                    $.getJSON("/OutwardToTailor/GetSalesBillFromClient", { ClientName: client },
               function (data) {

                   var select = $("#SalesBillNoDDL");
                   select.empty();
                   $.each(data.SalesBillNos, function (index, itemdata) {
                       select.append($('<option/>', {
                           text: itemdata.Text,
                           value: itemdata.Value,
                       }));
                   });
               });


                    $.getJSON("/OutwardToTailor/GetClientDetails", { name: client },
                        function (data) {
                            document.getElementById("OutwardToTailorDetails_ClientAddress").value = data.Address;
                            document.getElementById("OutwardToTailorDetails_ClientContact").value = data.ContactNo1;
                            document.getElementById("OutwardToTailorDetails_ClientEmail").value = data.Email;
                        });

                }
            }
        });
    });
</script>

