            var advpay = document.getElementById("AdvancePayment").value;
            if (advpay == 0 || advpay == "") {
                alertbox("Advance Payment Can Not Be Zero Or Empty..");
                validationcounter++;
                return false;
            }
            if ($("#ClientName").val() == "" || $("#ddltypeofmode").val() == "" || $("#ddltransportnames").val() == "" || $("#SalesOrderDetails_DeliveryDate").val() == "" || $("#SalesMan").val() == "") {
                validationcounter++;
            }

